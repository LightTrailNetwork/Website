mber":6,"content":[{"text":"Now I know that the LORD saves His anointed;","poem":1},{"text":"He answers him from His holy heaven","poem":2},{"lineBreak":true},{"text":"with the saving power of His right hand.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"Some trust in chariots and others in horses,","poem":1},{"text":"but we trust in the name of the LORD our God.","poem":2}]},{"type":"verse","number":8,"content":[{"text":"They collapse and fall,","poem":1},{"text":"but we rise up and stand firm.","poem":2}]},{"type":"verse","number":9,"content":[{"text":"O LORD, save","poem":1},{"noteId":43},{"text":"the king.","poem":1},{"text":"Answer us on the day we call.","poem":2}]}],"footnotes":[{"noteId":43,"caller":"+","text":"Or give victory to","reference":{"chapter":20,"verse":9}}]},"thisChapterLink":"/api/BSB/PSA/20.json","thisChapterAudioLinks":{"gilbert":"https://openbible.com/audio/gilbert/BSB_19_Psa_020_G.mp3","hays":"https://openbible.com/audio/hays/BSB_19_Psa_020_H.mp3","souer":"https://openbible.com/audio/souer/BSB_19_Psa_020.mp3"},"nextChapterApiLink":"/api/BSB/PSA/21.json","nextChapterAudioLinks":{"gilbert":"https://openbible.com/audio/gilbert/BSB_19_Psa_021_G.mp3","hays":"https://openbible.com/audio/hays/BSB_19_Psa_021_H.mp3","souer":"https://openbible.com/audio/souer/BSB_19_Psa_021.mp3"},"previousChapterApiLink":"/api/BSB/PSA/19.json","previousChapterAudioLinks":{"gilbert":"https://openbible.com/audio/gilbert/BSB_19_Psa_019_G.mp3","hays":"https://openbible.com/audio/hays/BSB_19_Psa_019_H.mp3","souer":"https://openbible.com/audio/souer/BSB_19_Psa_019.mp3"},"numberOfVerses":9}